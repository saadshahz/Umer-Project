{
    "version": 2,
    "builds": [
        {
            "src": "backend/app.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "../frontend/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/backend/app.py"
        }
    ]
}